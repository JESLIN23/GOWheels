{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
//     "no-const-assign": "error",
//     "no-multiple-empty-lines": "off",
//     "newline-after-import": 0,
//     "no-console": 0
        "react/prop-types": "off"
    }
}
